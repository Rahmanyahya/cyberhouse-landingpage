---
import '@fontsource-variable/inter';
import Compatibility from '~/components/Partner.astro';
import Benefit from '~/components/Benefit.astro';
import Footer from '~/components/footer.astro';
import Header from '~/components/header.astro';
import Bonus from '~/components/Bonus.astro';
import Pricing from '~/components/pricing.astro';
import Home from '~/components/Home.astro';
import Testimoni from '~/components/Testimoni.astro';
import Kurikulum from '~/components/kurikulum.astro';
import Mentor from '~/components/mentor.astro'
import Testimoni2 from '~/components/Testimoni_2.astro';
import FAQ from '~/components/faq.astro';
import '~/styles/index.css';
---

<!doctype html>
<html
	lang="en"
	class=" h-full motion-safe:scroll-smooth overflow-x-hidden "
	data-theme="dark">
	<head>
		<title>Cyber House</title>
		<meta charset="utf-8" />
		<meta
			name="viewport"
			content="width=device-width"
		/>
	</head>
	<body
		class="h-full overflow-x-hidden bg-default text-default text-base selection:bg-secondary selection:text-white">
		<Header />
		<Home />
		<div class="space-y-24  py-32">
			<Benefit />
			<Testimoni />
			<Kurikulum />
			<Mentor/>
			<Bonus/>
			<Pricing />
			<Testimoni2/>
			<FAQ />
			<Compatibility />
		</div>
		<Footer />
	</body>
</html>
